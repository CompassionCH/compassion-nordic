<odoo>
    <data>
        <record model="ir.ui.view" id="disbursement_data_tree_view">
            <field name="name">Disbursement Data Tree View</field>
            <field name="model">disbursement.data</field>
            <field name="arch" type="xml">
                <tree string="Disbursement Data">
                    <field name="company"/>
                    <field name="month"/>
                    <field name="account"/>
                    <field name="fund"/>
                    <field name="debit"/>
                    <field name="credit"/>
                    <field name="amount"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="disbursement_data_form_view">
            <field name="name">Disbursement Data Form View</field>
            <field name="model">disbursement.data</field>
            <field name="arch" type="xml">
                <form string="Disbursement Data">
                    <sheet>
                        <group>
                            <field name="company"/>
                            <field name="month"/>
                            <field name="account"/>
                            <field name="fund"/>
                            <field name="debit"/>
                            <field name="credit"/>
                            <field name="amount"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="disbursement_data_action">
            <field name="name">Disbursement Data</field>
            <field name="res_model">disbursement.data</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="Disbursement Data" id="menu_disbursement_data"
            action="disbursement_data_action"
            sequence="20"
            groups="billing.group_billing_administrator"/>
    </data>
</odoo>